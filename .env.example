# Example environment configuration for E2E tests
# Copy this file to .env and modify values as needed

# Application URLs
FRONTEND_URL=http://localhost:8888
API_URL=http://localhost:8888

# Test credentials
TEST_USER_EMAIL=admin@forge.local
TEST_USER_PASSWORD=admin123

# Test configuration
NODE_ENV=test
TEST_TIMEOUT=30000

# Debug configuration
DEBUG=false
VERBOSE=false

# Browser configuration (for Puppeteer tests)
HEADLESS=true
BROWSER_TIMEOUT=30000

# CI/CD configuration
CI=false
COVERAGE=false